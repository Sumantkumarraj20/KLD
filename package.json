{
  "name": "kids-learning-dashboard",
  "version": "2.5.0",
  "description": "Family Points System - Kids Learning Dashboard with Next.js and Tailwind CSS",
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "export": "NODE_ENV=production next build && next export",
    "deploy": "echo 'Ready to push ./out to gh-pages branch'",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "25.2.1",
    "@types/react": "19.2.13",
    "create-next-app": "^16.1.6",
    "gh-pages": "^6.3.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.24",
    "lucide-react": "^0.563.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
